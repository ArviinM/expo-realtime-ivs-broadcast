{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "expo-realtime-ivs-broadcast": [
        "../src/index"
      ],
      "expo-realtime-ivs-broadcast/*": [
        "../src/*"
      ],
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
